<p id="notice"><%= notice %></p>

<h1>Icds</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @icds.each do |icd| %>
      <tr>
        <td><%= icd.code %></td>
        <td><%= icd.title %></td>
        <td><%= link_to 'Show', icd %></td>
        <td><%= link_to 'Edit', edit_icd_path(icd) %></td>
        <td><%= link_to 'Destroy', icd, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Icd', new_icd_path %>
