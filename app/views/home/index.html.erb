              <h1>
                Servicing Hours
              </h1>
              <p>
                We are always on ground at all service hours to handle all kinds of medical emergencies that may arise.
              </p>
              <ul class="time-list">
                <li class="d-flex justify-content-between">
                  <span>Sunday</span>
                  <span>08.00 am - 11.15 am</span>
                </li>
                <li class="d-flex justify-content-between">
                  <span>Wednesday</span>
                  <span>08.00 am - 10.00 pm</span>
                </li>
                <li class="d-flex justify-content-between">
                  <span>All Special Services</span>
                  <span>Start - Finish</span>
                </li>                               
              </ul>
            </div>
            <div class="col-lg-6 col-md-6 appointment-right pt-60 pb-60">

              <%= form_for(@appointment, :html =>{:class => "form-wrap"}) do |f| %>
                <% if @appointment.errors.any? %>
                  <div id="error_explanation">
                    <h2><%= pluralize(@appointment.errors.count, "error") %> prohibited this appointment from being saved:</h2>

                    <ul>
                    <% @appointment.errors.full_messages.each do |message| %>
                      <li><%= message %></li>
                    <% end %>
                    </ul>
                  </div>
                <% end %>

                  <%= f.text_field :name, :class => "form-control", :placeholder => "Patient Name" %>
                  <%= f.text_field :phone, :class => "form-control", :placeholder => "Phone Number" %>
                  <%= f.text_field :email, :class => "form-control", :placeholder => "Email Address" %>
                  <%= f.date_field :date_of_birth, class: "form-control", placeholder: "Date of Birth"%>                
                  <%= f.collection_select :disease_id, Disease.order(:name), :id, :name, {},{class: "form-control"} %>
                  <%= f.date_select :appointment_date, class: "form-control", placeholder: "Appointment Date"%>              
                  <%= f.text_area :message, :class => "form-control", :placeholder => "Message" , cols: 20, rows: 10%>                  
                  <%= f.submit "Confirm Booking", :class => "primary-btn text-uppercase" %>
              <% end %>
            </div>
          </div>
        </div>  
      </section>
